{# lib/Resources/views/steps/02-layout/02-empty.html.twig #}
{% extends '@MazariniDesign/layout.html.twig' %}

{% block title %}
    Empty
{% endblock %}

{% block footer %}
    <h1>Title 1</h1>
    <h2>Title 2</h2>
    <h3>Title 3</h3>
    <h4>Title 4</h4>
    <h5>Title 5</h5>
    <h6>Title 6</h6>
{% endblock footer %}

{% block first_bar %}
    {{ block('footer') }}
{% endblock first_bar %}

{% block second_bar %}
    {{ block('footer') }}
{% endblock second_bar %}

{% block header %}
    {{ block('footer') }}
{% endblock header %}

{% block page %}
    {{ block('footer') }}
{% endblock page %}

{% block navbar %}
    {% include '@MazariniDesign/empty/navbar.html.twig' %}
{% endblock navbar %}

{% block design_modal %}
    {% embed '@MazariniDesign/tool/modal.html.twig' %}
        {% block message %}
            <p>This page presents texts in the differents parts of the page</p>
        {% endblock %}
    {% endembed %}
{% endblock %}
